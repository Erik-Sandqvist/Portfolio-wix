 <link rel="stylesheet" href="/oneproject.css" />
<div class="style">
  <h1>{{project.pname}}</h1>
  <p>{{project.pdesc}}</p>
  <p>Year: {{project.pyear}}</p>
  <p>Type of project: {{project.ptype}}</p>
  <p>Skills associated with this project:</p>
  <ul>
    {{#each skills}}
      <li>{{sname}} (Skill ID: {{sid}})</li>
    {{/each}}
  </ul>
  <br>
  <img
    src="{{project.pimageurl}}"
    alt="Image of {{project.pname}}"
    style="width:80%; height:auto; margin: 0 auto; display: block;"
  />

</div>
